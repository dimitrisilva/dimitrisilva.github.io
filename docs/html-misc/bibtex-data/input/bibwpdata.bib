@article{saavedra2023voting,
  title = {Voting Rights and Media Sentiment: Evidence from Early Suffrage States},
  author = {Saavedra, Martin Hugo},
  journal = {The Journal of Economic History},
  volume = {},
  number = {},
  pages = {},
  year = {2023},
  url = {},
  doi = {},
  publisher = {Cambridge University Press},
  abstract = {Did women's suffrage affect media sentiment towards voting rights and narratives about women more generally? I identify pro- and anti-suffrage language using publications that explicitly argued for or against early voting reform. I then measure media sentiment using language in newspapers and topic modeling to identify common themes about either suffrage or women. Difference-in-differences estimates show that newspapers discussed suffrage when the issue was salient and that newspaper sentiment moved in opposition to the status quo, with average sentiment becoming more anti-suffrage following early suffrage laws. Lastly, suffrage increased discussions of women in politics for several years.}
}

