@article{egerod2024many,
  title = {How many is enough? Sample size in staggered difference-in-differences designs},
  author = {Egerod, Benjamin and Hollenbach, Florian M.},
  year = {2024},
  url = {https://osf.io/preprints/osf/ac5ru_v1},
  doi = {10.31219/osf.io/ac5ru},
  abstract = {We use power simulations to examine when difference-in-differences techniques for staggered treatments provide informative estimates. Using data on drug deaths in US States, we show that effects of 10% are necessary before the statistical tests achieve 80% power. Conditional on statistical significance, when interventions generate a 5% effect, difference-in-differences techniques overestimate the true effect by close to 100%, on average. We use data on publicly traded firms to investigate how the necessary sample size differs by context. More than 250 firms are needed to study interventions generating a 5% increase in logged revenue, while several thousand are needed to study similar increases in return on assets. Our paper shows that: i) surprisingly large sample sizes may be required to study meaningful interventions, ii) power simulations will reveal which interventions can be studied with the data at hand, and iii) power may be better for alternative measures of similar concepts.}	
}


